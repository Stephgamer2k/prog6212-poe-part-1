<Window x:Class="Contract_Monthly_Claim_System_PoE_part1.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Contract_Monthly_Claim_System_PoE_part1"
        mc:Ignorable="d"
        Title="Login" Height="517" Width="522">
    <Canvas>
        <Label Content="Login Window" Canvas.Left="164" Canvas.Top="28" Height="48" Width="196" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="25" FontWeight="Bold"/>
        <Label Content="Email:" Canvas.Left="126" Canvas.Top="144" FontSize="16"/>
        <Label Content="Password" Canvas.Left="126" Canvas.Top="225" FontSize="16"/>
        <TextBox x:Name="TextBox_Email" Canvas.Left="126" TextWrapping="Wrap" Canvas.Top="180" Width="159" Height="27"/>
        <PasswordBox Canvas.Left="126" Canvas.Top="256" Width="159" Height="27"/>
        <Button x:Name="btn_Login" Content="Login" Canvas.Left="186" Canvas.Top="352" Height="51" Width="152" HorizontalAlignment="Left" VerticalAlignment="Center" FontWeight="Bold" FontSize="23"/>

    </Canvas>
</Window>
